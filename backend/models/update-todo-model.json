{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 100
    },
    "dueDate": {
      "type": "string",
      "minLength": 4
    },
    "done": {
      "type": "boolean",
      "minLength": 4,
      "maxLength": 4
    }
  },
  "required": [
    "name",
    "dueDate",
    "done"
  ],
  "additionalProperties": false
}
